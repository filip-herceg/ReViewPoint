# Test Environment Configuration Example
# Copy this file to .env.test and customize for your testing setup
NODE_ENV=test

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=5000
VITE_WS_URL=ws://localhost:8000/api/v1

# Monitoring Configuration
VITE_SENTRY_DSN=
VITE_ENABLE_ANALYTICS=true
VITE_LOG_LEVEL=error
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Application Metadata
VITE_APP_VERSION=0.1.0-test
VITE_APP_NAME=ReViewPoint (Test)

# Feature Flags (test configuration)
# All features enabled for comprehensive testing
VITE_FEATURE_ENABLE_PASSWORD_RESET=true
VITE_FEATURE_ENABLE_SOCIAL_LOGIN=true
VITE_FEATURE_ENABLE_TWO_FACTOR_AUTH=true
VITE_FEATURE_ENABLE_MULTIPLE_FILE_UPLOAD=true
VITE_FEATURE_ENABLE_DRAG_DROP_UPLOAD=true
VITE_FEATURE_ENABLE_UPLOAD_PROGRESS=true
VITE_FEATURE_ENABLE_FILE_PREVIEW=true
VITE_FEATURE_ENABLE_AI_REVIEWS=true
VITE_FEATURE_ENABLE_COLLABORATIVE_REVIEWS=true
VITE_FEATURE_ENABLE_REVIEW_COMMENTS=true
VITE_FEATURE_ENABLE_REVIEW_EXPORT=true
VITE_FEATURE_ENABLE_DARK_MODE=false
VITE_FEATURE_ENABLE_NOTIFICATIONS=false
VITE_FEATURE_ENABLE_BREADCRUMBS=true
VITE_FEATURE_ENABLE_SIDEBAR=false
VITE_FEATURE_ENABLE_WEBSOCKET=false
VITE_FEATURE_ENABLE_VIRTUALIZATION=false
VITE_FEATURE_ENABLE_LAZY_LOADING=false
VITE_FEATURE_ENABLE_CODE_SPLITTING=false
VITE_FEATURE_ENABLE_ANALYTICS=false
VITE_FEATURE_ENABLE_ERROR_REPORTING=false
VITE_FEATURE_ENABLE_PERFORMANCE_MONITORING=false
VITE_FEATURE_ENABLE_WEB_VITALS=false
VITE_FEATURE_ENABLE_DEV_TOOLS=false
VITE_FEATURE_ENABLE_DEBUG_MODE=false
VITE_FEATURE_ENABLE_TEST_MODE=true
